<Page x:Class="Sunum.Sayfalarİki.Olaylar.MainTDS"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sunum.Sayfalarİki.Olaylar"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainTDS">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="7*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Background="Gray" >
            <Button x:Name="btnTakip" Content="Takip Olayı" Height="40" FontSize="20" Margin="2,3" BorderThickness="0"  Foreground="DarkOrange" FontWeight="DemiBold"  />
            <Button x:Name="btnDm" Content="DM Olayı" Height="40" FontSize="20" Margin="2,3" BorderThickness="0" Foreground="DarkOrange" FontWeight="DemiBold"  />
        </StackPanel>
        <Frame Grid.Row="1" x:Name="frmmw" NavigationUIVisibility="Hidden" Source="\Sayfalarİki\Olaylar\DmOlayı.xaml"/>
    </Grid>
</Page>
